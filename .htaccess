<IfModule mod_rewrite.c>
   RewriteEngine on
   RewriteCond    %{REQUEST_URI} (js/core|css/core|img/core|fonts/core)
   RewriteRule    ^(.*)$ greyback_core/webroot/$1 [L]
   RewriteCond    %{REQUEST_URI} !greyback_core/webroot
   RewriteRule    ^$ webroot/    [L]
   RewriteCond    %{REQUEST_URI} !greyback_core/webroot
   RewriteRule    (.*) webroot/$1 [L]
</IfModule>